<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-odkconfigurator" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>odkConfigurator</name>
    <js-module name="odkConfigurator" src="www/odkConfigurator.js"><clobbers target="cordova.plugins.odkConfigurator"/></js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="odkConfigurator"><param name="android-package" value="cordova_plugin_odkconfigurator.odkConfigurator"/></feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/odkConfigurator.java" target-dir="src/cordova_plugin_odkconfigurator/odkConfigurator"/>
    </platform>
</plugin>
